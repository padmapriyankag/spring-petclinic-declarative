pipeline{
    agent any
    stages {
        stage('test'){
            steps{
            sh 'echo hello'
        }
        }
        stage('git clone'){
            agent{
                    label 'NODE1_LABEL'
                }
            steps{
                git url: 'https://github.com/spring-projects/spring-petclinic.git',
                branch : 'main'
            }
        }
        stage('creating package'){
            agent{
                label 'NODE1_LABEL'
            }
            steps{
                sh 'mvn package'
            }
        }
    }
  
    
}
